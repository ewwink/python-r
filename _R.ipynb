   "source": [
    "library('rvest')\n",
    "library('xml2')\n",
    "\n",
    "url <- 'https://www.google.com/search?q=tasikmalaya'\n",
    "googleurl <- read_html(url)\n",
    "address <- html_nodes(googleurl,'.r')\n",
    "\n",
    "url_ <- address %>%\n",
    "  html_nodes(\"a\") %>%\n",
    "  html_attr(\"href\")\n",
    "address <- html_text(address)\n",
    "print(url_)\n",
    "print(address)"
   ]
  }
